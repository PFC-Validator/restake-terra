{
  "name": "cra-template",
  "version": "1.0.0",
  "scripts": {
    "build": "react-app-rewired build",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --colors",
    "coverage": "react-app-rewired test --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "cross-env CI=true react-scripts test --colors --findRelatedTests"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "browserslist": {
    "production": [
      "chrome > 60",
      "firefox > 60",
      "safari > 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@terra-money/terra.js": "=3.0.6",
    "@terra-money/wallet-provider": "3.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-query": "^3.34.4",
    "react-icons": "^4.3.1",
    "bignumber.js": "^9.0.2",
    "@arthuryeti/terra": "2.1.0",
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "styled-components": "^5.3.3",
    "browserify": "^17.0.0",
    "path": "0.12.7",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@arthuryeti/tsconfig": "1.0.0",
    "eslint-preset-arthuryeti": "1.0.0",
    "eslint": "8.7.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "lint-staged": "^11.2.6",
    "process": "^0.11.10",
    "react-app-rewired": "^2.1.11",
    "react-scripts": "5.0.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.6.2",  "prettier": "^2.5.1"
  }
}
